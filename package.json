{
  "name": "arcsat-crm",
  "version": "1.0.0",
  "description": "Sistema de Gestão Empresarial - ArcSat",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "dev:debug": "tsx watch --inspect src/server.ts",
    "dev:ai": "node src/server-ai.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ai": "node test-azure-ai.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write 'src/**/*.{ts,json}'",
    "typecheck": "tsc --noEmit",
    "frontend:dev": "cd frontend/arcsat-landing && npm run dev",
    "frontend:build": "cd frontend/arcsat-landing && npm run build",
    "docs:serve": "docsify serve docs",
    "setup:azure-ai": "node setup-azure-ai.js",
    "azure:deploy-models": "node setup-azure-ai.js",
    "azure:health-check": "curl http://localhost:5000/health",
    "db:init": "tsx scripts/init-database.ts",
    "db:test": "tsx scripts/test-mongodb.ts",
    "db:health": "tsx scripts/test-mongodb.ts",
    "db:seed": "tsx scripts/init-database.ts",
    "db:setup": "tsx scripts/setup-mongodb.ts",
    "domains:setup": "node scripts/setup-domains.cjs",
    "domains:check": "node scripts/fix-docs-domain.cjs",
    "domains:test": "node scripts/test-domains.cjs",
    "domains:report": "type domain-setup-report.json",
    "mongodb:test": "tsx scripts/test-mongodb.ts",
    "mongodb:setup": "tsx scripts/setup-mongodb.ts",
    "mcp:setup": "powershell -ExecutionPolicy Bypass -File setup-mcp-enterprise.ps1",
    "mcp:setup-startup": "powershell -ExecutionPolicy Bypass -File setup-mcp-enterprise.ps1 -Tier startup",
    "mcp:setup-growth": "powershell -ExecutionPolicy Bypass -File setup-mcp-enterprise.ps1 -Tier growth",
    "mcp:setup-enterprise": "powershell -ExecutionPolicy Bypass -File setup-mcp-enterprise.ps1 -Tier enterprise",
    "mcp:test": "powershell -ExecutionPolicy Bypass -File setup-mcp-enterprise.ps1 -TestConnections",
    "enterprise:startup": "npm run mcp:setup-startup && npm run setup:azure-ai",
    "enterprise:growth": "npm run mcp:setup-growth && npm run setup:azure-ai && npm run test:ai",
    "enterprise:full": "npm run mcp:setup-enterprise && npm run setup:azure-ai && npm run test:ai",
    "business:intelligence": "node scripts/run-bi-analysis.js",
    "automation:test": "node scripts/test-automation-workflows.js",
    "industrial:lean": "powershell -ExecutionPolicy Bypass -File setup-consultoria-industrial.ps1 -Especializacao lean_manufacturing",
    "industrial:iso": "powershell -ExecutionPolicy Bypass -File setup-consultoria-industrial.ps1 -Especializacao iso_compliance",
    "industrial:40": "powershell -ExecutionPolicy Bypass -File setup-consultoria-industrial.ps1 -Especializacao industria_40",
    "industrial:safety": "powershell -ExecutionPolicy Bypass -File setup-consultoria-industrial.ps1 -Especializacao seguranca_trabalho",
    "industrial:setup-complete": "powershell -ExecutionPolicy Bypass -File setup-consultoria-industrial.ps1 -SetupCompleto",
    "industrial:test": "powershell -ExecutionPolicy Bypass -File setup-consultoria-industrial.ps1 -TestIntegracoes",
    "industrial:start": "copy .env.industrial .env && node src/gestao-clientes-industrial.js",
    "industrial:dashboard": "start http://localhost:5000/dashboard && npm run industrial:start",
    "sefaz:test": "node scripts/test-sefaz.js",
    "sefaz:docs": "echo 'SEFAZ/ReceitaWS - Consulta automática de dados empresariais via CNPJ'",
    "dev:industrial-consulting": "node src/server-industrial.js",
    "test:industrial": "node test-industrial-modules.js",
    "health:industrial-modules": "curl http://localhost:5000/api/industrial/health",
    "multi-tenant:setup": "node scripts/setup-multi-tenant.js",
    "compliance:check": "node scripts/compliance-audit.js",
    "performance:monitor": "node scripts/performance-monitor.js"
  },
  "keywords": [
    "crm",
    "erp",
    "gestao",
    "empresarial",
    "azure",
    "nodejs",
    "mongodb"
  ],
  "author": "Ávila Inc <nicolas@avila.inc>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@azure/core-auth": "^1.10.1",
    "@azure/identity": "^4.13.0",
    "@azure/openai": "^2.0.0",
    "applicationinsights": "^2.9.5",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.3.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.2",
    "morgan": "^1.10.0",
    "validator": "^13.12.0",
    "winston": "^3.14.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.0",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.12.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avilaops/ArcSat.git"
  }
}
